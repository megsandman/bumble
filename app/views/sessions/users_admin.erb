<div>
  <h2 class="sub-header">Users:</h2>
  <div class="table-responsive" id="user-table">
    <table class="table table-striped">
      <thead>
        <tr>
          <th>User Name</th>
          <th>Email</th>
          <th>Products Viewed</th>
          <th>Access</th>
          <th>Edit Access</th>
          <tr>
          </thead>
          <tbody>
            <% @users.each do |user|%>
            <tr id="<%= user.id %>">
              <td><%= user.name %></td>
              <td><%= user.email %></td>
              <td><%= user.actions.count %></td>
              <td><%= user.admin %></td>
              <td><form method="post" action="/admin/users/<%= user.id %>/edit_access"><input type="submit" class="btn btn-default edit-access" id="<%= user.id %>" value="Edit Access"></form></td>
            </tr>
            <% end %>
          </tbody>
          <table>
          </div>
        </div>



